<?xml version="1.0" encoding="UTF-8"?>

<?import biz.ostw.security.asn1.editor.ui.control.Asn1View?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="biz.ostw.security.asn1.editor.ui.Main">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="72.0" layoutY="72.0" style="-fx-background-color: #fcffea;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <MenuBar fx:id="menuBar">
                    <menus>
                        <Menu mnemonicParsing="false" text="%window.menubar.file.tilte">
                            <items>
                                <MenuItem fx:id="fileOpenMenuItem" mnemonicParsing="false" text="%window.menubar.file.open.tilte" />
                                <MenuItem fx:id="fileSaveAsMenuItem" mnemonicParsing="false" text="%window.menubar.file.saveas.tilte" />
                                <MenuItem fx:id="closeMenuItem" mnemonicParsing="false" text="%window.menubar.file.close.tilte" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="%window.menubar.help.title">
                            <items>
                                <MenuItem fx:id="aboutMenuItem" mnemonicParsing="false" text="%window.menubar.help.about.title" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <Asn1View fx:id="asn1View" id="star" VBox.vgrow="ALWAYS" />
            </children>
        </VBox>
    </children>
</AnchorPane>

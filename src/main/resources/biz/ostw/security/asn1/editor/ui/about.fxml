<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<fx:root type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.172-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button fx:id="closeButton" layoutX="225.0" layoutY="257.0" mnemonicParsing="false" onAction="#onCloseClick"
                text="%window.about.button.close.title"/>
        <Label fx:id="mainLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="65.0" layoutY="44.0"
               prefHeight="163.0" prefWidth="377.0" text="%window.about.mainLabel" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="36.0"/>
            </font>
        </Label>
        <Label fx:id="authorLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="2.0" layoutY="207.0"
               prefHeight="16.0" prefWidth="506.0" text="%window.about.authorLabel" textAlignment="CENTER"/>
    </children>
</fx:root>
